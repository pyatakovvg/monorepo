{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    ["@babel/preset-typescript", {
      "allowDeclareFields": true,
      "onlyRemoveTypeImports": true
    }]
  ],
  "plugins": [
    ["module-resolver", {
      "alias": {
        "@/types": "./src/types.js",
        "@/utils": "./src/utils",
        "@/symbols": "./src/symbols",
        "@/typography": "./src/typography"
      }
    }]
  ]
}